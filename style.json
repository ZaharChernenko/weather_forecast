			}],
			"style": [{
				"whenMenuIsCollapsed": [
					"border: 2px solid transparent"
				],
				"whenMenuIsExpanded": [
					"border: 2px solid rgb(9, 5, 13)"
				]
			}]
		}],
		"menuTransitionAnimation": [{
			"animationDuration": 1000,
			"animationEasingCurve": "OutBack"
		}]
	}]
}