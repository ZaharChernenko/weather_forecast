{
    "ShowLogs": true,
    "QMainWindow": [
        {
            "tittle": "\u041f\u0440\u043e\u0433\u043d\u043e\u0437 \u043f\u043e\u0433\u043e\u0434\u044b"
        }
    ],
    "QSettings": [
        {
            "ThemeSettings": [
                {
                    "CustomTheme": []
                }
            ]

        }
    ],
    "QCustomSlideMenu": [{
		"name": "left_menu",
		"defaultSize": [{
			"width": 250,
			"height": "auto"
		}],
		"collapsedSize": [{
			"width": 0,
			"height": "parent"
		}],
		"expandedSize": [{
			"width": 250,
			"height": "auto"
		}],
		"toggleButton": [{
			"buttonName": "pushButton",
